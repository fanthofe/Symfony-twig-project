<!DOCTYPE html>
<html lang="fr" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg" data-sidebar-image="none" data-preloader="disable">

    <head>
        <meta charset="UTF-8">
        {{ include('partials/title-meta.html.twig', {title: 'Theme Tristan'}) }}

        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <style data-fullcalendar></style>
            <link href="/css/app.min.css" rel="stylesheet" type="text/css" />

            <link href="/css/icons.min.css" rel="stylesheet" type="text/css">
            <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css">
            <link href="/css/app.min.css" rel="stylesheet" type="text/css" />
            <link href="/css/custom.min.css" rel="stylesheet" type="text/css" />
            <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet">

            <link href="https://cdn.datatables.net/v/bs5/dt-1.13.6/b-2.4.1/r-2.5.0/datatables.min.css" rel="stylesheet">
            <link href="https://cdn.datatables.net/1.13.7/css/jquery.dataTables.css" rel="stylesheet">

            <!-- jsvectormap css -->
            <link href="/libs/jsvectormap/css/jsvectormap.min.css" rel="stylesheet" type="text/css" />
            <link rel="stylesheet" href="/libs/flatpickr/flatpickr.min.css"type="text/css"  />
            <!--Swiper slider css-->
            <link href="/libs/swiper/swiper-bundle.min.css" rel="stylesheet" type="text/css" />
            <link href="https://unpkg.com/gridjs/dist/theme/mermaid.min.css" rel="stylesheet" />
            
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
            <!-- quill css -->
            <link href="/libs/quill/quill.core.css" rel="stylesheet" type="text/css" />
            <!-- bubble css for bubble editor-->
            <link href="/libs/quill/quill.bubble.css" rel="stylesheet" type="text/css" />
            <!-- snow css for snow editor-->
            <link href="/libs/quill/quill.snow.css" rel="stylesheet" type="text/css" />

             
            {{ include('partials/head-css.html.twig') }}
        {% endblock %}    

        <script 
            src="https://code.jquery.com/jquery-3.7.1.min.js"
            integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo="
            crossorigin="anonymous">
        </script>     

        <script type="text/javascript" src="/libs/flatpickr/flatpickr.min.js"></script>
        <script src='https://cdn.jsdelivr.net/npm/fullcalendar-scheduler@6.1.10/index.global.min.js'></script>
        <script src='https://cdn.jsdelivr.net/npm/fullcalendar@6.1.10/index.global.min.js'></script>
        <script src="/libs/bootstrap/js/bootstrap.min.js"></script>
        <script src="https://cdn.datatables.net/v/bs5/dt-1.13.6/b-2.4.1/r-2.5.0/datatables.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/i18n/fr.js"></script>
        <script src="https://npmcdn.com/flatpickr/dist/flatpickr.min.js"></script>
        <script src="https://npmcdn.com/flatpickr/dist/l10n/fr.js"></script>
    </head>

    <body>
        <!-- Begin page -->
        <div id="layout-wrapper">

            <!-- Header -->
            {{ include('custom/menu/header.html.twig') }}
            <!-- End Header -->

            <!-- ============================================================== -->
            <!-- Start Content page with menu (container) -->
            <!-- ============================================================== -->

            {% block body %}
            {% endblock %}

            <!-- footer -->
            {{ include('custom/menu/footer.html.twig') }}
            <!-- end Footer -->

        </div>

        {{ include('partials/customizer.html.twig') }}
    </body>

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ include('partials/vendor-scripts.html.twig') }}
          
        <script src="/libs/prismjs/prism.js"></script>
        <script src="/libs/quill/quill.min.js"></script>
        <script type="text/javascript" src="/libs/choices.js/public/assets/scripts/choices.min.js"></script>

        <!-- App js -->
        <script src="/js/app.js"></script>
    {% endblock %}

</html>